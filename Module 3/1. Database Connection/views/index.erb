<h1>Items</h1>

<table border=1>
    <tr>
        <th>Name</th>
        <th>Price</th>
            <th>Category</th>
    </tr>
    <% items.each do |item| %>
    <tr>
        <td><%= item.name %></td>
        <td><%= item.price %></td>
        <td><%= item.category %></td>
    </tr>
    <% end %>
</table>

<br>
<a href='/items/new'>New Item</a>